
#include <iostream>
using  namespace std;
void multiplicationTable();
int main() {
   
    multiplicationTable();
    
    return 0;
}

void multiplicationTable(){
    int i, m;
    cout<<"Please enter a Number: ";
    cin>>m;
    for(i=0; i<=m; i++){
        cout<<m<<" x "<<i<<" : "<<i*m<<endl;
        
    }
    
}
