#include <iostream>

using namespace std;

int main()
{
    int age, birth_year, current_year;

    cout << "Please enter your age: ";
    cin >> age;

    current_year = 2023; //assing a value to our variable
    birth_year = current_year - age; //calculates the birth year

    cout << "You were born in " << birth_year;

    return 0;
}
