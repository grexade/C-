
#include <iostream>
using namespace std;
int main() {

    
    int n;
    int factorial = 1;

cout << "Enter a number to calculate factorial: ";
cin >> n;

    for(int i = 1; i <=n; ++i)
    {
        //cout<<i;
        factorial *= i;  
    }

    cout << "Factorial of " << n << " = " << factorial; 

    return 0;
}
