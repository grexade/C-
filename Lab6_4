#include <iostream>
using namespace std;
int main(void){
	int myArray[5],i;
	int key,x;
	
	for(i=0; i<5;i++){
		cout<<"Enter number"<<i+1<<" : ";
		cin>>myArray[i];		
	}
	cout<<"enter a key Number: ";
	cin>>key;
	
	for(i=0; i<5;i++){
		if( myArray[i]==key)
			 {x=1;
			 break;}
			
	}
	
	if(x==1){
		cout<<"key is found";
	}
	else
	cout<<"key not found";
	
	
	return 0;
}




#include <iostream>
using namespace std;

int main() {
    int num[5];
    int key,n;

    

    for (n=0; n<5; n++) {
        cout << "Enter Number " <<n+1<< ": ";
        cin >> num[n];
    }

    cout << "Enter the key number: ";
    cin >> key;

    bool keyFound = false;

    for (n=0; n<5; n++) {
        if (num[n] == key) {
            keyFound = true;
            break;
        }
    }

    if (keyFound) {
        cout << "Key is found\n";
    } else {
        cout << "Key is not found";
    }

    return 0;
}
