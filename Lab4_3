#include <iostream>
#include <cmath>
using namespace std;
int main()
{

    int w, x, y, z;
    float sum;
    cout << "Please enter a value to w : ";
    cin >> w;
    cout << "Please enter a value to x : ";
    cin >> x;
    cout << "Please enter a value to y : ";
    cin >> y;
    cout << "Please enter a value to z : ";
    cin >> z;

    sum = pow(w, x) + pow(y, 2) + exp(z);

    cout << "The result is " << sqrt(sum);

    return 0;
}
