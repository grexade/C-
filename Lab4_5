#include <iostream>
#include <cmath>

using namespace std;

int main()
{
    int x,y,z;
    
   cout << "Please enter value of x : "; 
   cin>>x;
   cout << "Please enter value of y : "; 
   cin>>y;
   cout << "Please enter value of z : "; 
   cin>>z;
   
   float res = (2*x + 10*y) / log10(z);
   
   cout<<"Result is: "<<res;
  
   return 0;
}
