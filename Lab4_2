#include <iostream>
#include <cmath>
using namespace std;
int main()
{
    int a, b;
    float sum;
    cout << "Please enter a value to a : ";
    cin >> a;
    cout << "Please enter a value to b : ";
    cin >> b;

    sum = pow(a,2) + pow(b,3);

    cout << "The square root of " << sum << " is " << sqrt(sum);

    return 0;
}
