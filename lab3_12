#include <iostream>
using namespace std;

int main() {
   int num, max, count = 1;

   cout << "Enter number " << count << ": ";
   cin >> num;
   max = num;


   while (count <= 5) {
      cout << "Enter number " << count << ": ";
      cin >> num;
      if (num > max) {
         max = num;
      }
      count++;
   }

   cout << "The greatest number entered is: " << max << endl;

   return 0;
}
