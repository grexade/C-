#include <iostream>
#include <cmath>
using namespace std;
int main()
{
    int b, x;
    cout << "Please enter a value to w : ";
    cin >> b;
    cout << "Please enter a value to x : ";
    cin >> x;

    float nume = pow(2,b) + exp(4);
    float denom = log(x) + log10(5);

    cout << "The result is " << -1 * (nume / denom);

    return 0;
}
