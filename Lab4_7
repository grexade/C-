#include <iostream>
#include <cmath>
using namespace std;
int main()
{
    int a, b;
    cout << "Please enter the value of a : ";
    cin >> a;
    cout << "Please enter the value of b : ";
    cin >> b;
    
    float nume = exp(b);
    float deno = 2 * log10(exp(a)+b);
    
    float result =sqrt(nume/deno);
    
    cout<<"Result is "<<result<<endl;
    
    cout<<"smallest integer bigger than the result is "<<ceil(result)<<endl;
    cout<<"biggest integer less than the result is "<<floor(result);


    return 0;
}
